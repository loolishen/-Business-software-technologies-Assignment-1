<html>
<head>
  <title>List Events</title>
  <style>
    body {
      font-family: Arial, sans-serif;
    }
    table {
      border-collapse: collapse;
      width: 100%;
      margin-top: 20px;
    }
    th, td {
      border: 1px solid #dddddd;
      text-align: left;
      padding: 8px;
    }
    th {
      background-color: #f2f2f2;
    }
  </style>
</head>

<body>
    <h1>List Events</h1>
    <table>
      <tr>
        <th>ID</th>
        <th>Event Name</th>
        <th>Start Date Time</th>
        <th>Durtaion</th>
        <th>Category ID</th>
        <th>Event Description</th>
        <th>Event Image</th>
        <th>Capacity</th>
        <th>Tickets Available</th>
        <th>Ongoing</th>
      </tr>
      <% for (let i = 0; i < database.length; i++) { %>
        <tr>
            <td><%= database[i].id %></td>
          <td><%= database[i].eventName %></td>
          <td><%= database[i].startDateTime %></td>
          <td><%= database[i].duration %></td>
          <td><%= database[i].categoryId %></td>
          <td><%= database[i].eventDescription %></td>
          <td><%= database[i].eventImage %></td>
          <td><%= database[i].capacity %></td>
          <td><%= database[i].ticketsAvailable %></td>
          <td><%= database[i].isActive %></td>
        </tr>
      <% } %>
    </table>
  </body>
  <div class="container m-3">
    <a type="button" class="btn btn-primary" href='/'>Back</a>
    </h1>
    </div>
  </html>